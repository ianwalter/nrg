{
  "name": "@ianwalter/nrg",
  "description": "A batteries-included Node.js web framework",
  "version": "0.0.76",
  "author": {
    "name": "Ian Walter",
    "url": "https://ianwalter.dev"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bin": {
    "nrg": "cli.js"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "reset": "yarn nrg migrate && yarn nrg seed && yarn nrg run deleteAllEmail",
    "test": "NODE_ENV=test bff",
    "test:ci": "yarn reset && yarn test",
    "nrg": "node cli.js",
    "example:hello": "node examples/helloWorld.js",
    "example:uncaught": "node examples/uncaughtException.js",
    "example:error": "node examples/error.js",
    "example:warn": "node examples/warning.js",
    "example:prod": "NODE_ENV=production node examples/helloWorld.js | pino-print",
    "example:sessions": "node examples/sessions.js",
    "example:auth": "node examples/authRequired.js",
    "example:static": "node examples/static",
    "example:ssr": "node examples/ssr",
    "example:mq": "node examples/mq",
    "build:example:ssr": "NODE_ENV=production webpack examples/ssr",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "start:dc": "docker-compose up"
  },
  "homepage": "https://github.com/ianwalter/nrg#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/nrg.git"
  },
  "keywords": [
    "web",
    "framework",
    "node",
    "koa"
  ],
  "bugs": {
    "url": "https://github.com/ianwalter/nrg/issues"
  },
  "dependencies": {
    "@ianwalter/cli": "^4.1.0",
    "@ianwalter/cloneable": "^1.0.1",
    "@ianwalter/correct": "^1.1.0",
    "@ianwalter/merge": "^7.0.0",
    "@ianwalter/nrg-mq": "^0.0.2",
    "@ianwalter/nrg-router": "^0.4.0",
    "@ianwalter/nrg-session": "^6.0.0",
    "@ianwalter/pino-print": "^4.2.0",
    "@ianwalter/print": "^5.3.0",
    "@ianwalter/url": "^1.1.0",
    "bcrypt": "4.0.1",
    "date-fns": "^2.10.0",
    "devalue": "^2.0.1",
    "dot-prop": "^5.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "knex": "^0.20.11",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-csrf": "^3.0.8",
    "koa-json": "^2.0.2",
    "koa-pino-logger": "jstewmon/koa-pino-logger#cdfb9806508b7fc6e89e6a2ca1c92cd927ce6104",
    "koa-redis": "^4.0.1",
    "koa-send": "^5.0.0",
    "koa-webpack": "^5.3.0",
    "mailgen": "^2.0.10",
    "nanoid": "^2.1.11",
    "nodemailer": "^6.4.4",
    "objection": "^2.1.3",
    "pg": "^7.18.2",
    "pino": "^5.17.0",
    "pkg-dir": "^4.2.0",
    "read-pkg-up": "^7.0.1",
    "require-from-string": "^2.0.2",
    "server-destroy": "^1.0.1",
    "supertest": "^4.0.2",
    "webpack": "^4.42.0"
  },
  "devDependencies": {
    "@ianwalter/bff": "^9.6.0",
    "@ianwalter/eslint-config": "^4.0.0",
    "@ianwalter/release": "^5.0.0",
    "@ianwalter/renovate-config": "^1.2.0",
    "@ianwalter/requester": "^2.2.0",
    "babel-loader": "^8.0.6",
    "cheerio": "^1.0.0-rc.3",
    "html-webpack-plugin": "^3.2.0",
    "svelte": "^3.19.2",
    "svelte-loader": "^2.13.6",
    "vuepress": "^1.3.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "nrg": {
    "app": "examples/accounts"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter"
    ]
  },
  "eslintIgnore": [
    "dist"
  ],
  "renovate": {
    "extends": [
      "@ianwalter"
    ]
  },
  "release": {
    "test": "test:ci"
  }
}
